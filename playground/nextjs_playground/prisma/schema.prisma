generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model delete_me {
  id String @id @default(uuid()) @map("_id") @unique
}

model Product {
  id String @id @default(uuid()) @map("_id") @unique
  description String
  imageUrl String
  name String
  price Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  // table名の指定
  @@map("products")
}